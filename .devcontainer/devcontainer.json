{
  "image": "mcr.microsoft.com/devcontainers/python:3",
  "runArgs": [
    "--network",
    "als"
  ],
  "forwardPorts": [
    5000
  ],
  "features": {
    "ghcr.io/devcontainers/features/node:1": {
      "version": "latest"
    }
  },
  "extensions": [
    "ms-python.python",
  ],
  "settings": {
    "terminal.integrated.shell.linux": "/bin/bash"
  }
}